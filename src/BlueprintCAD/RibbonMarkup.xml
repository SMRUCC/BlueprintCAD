  <Application.Views>
    <Ribbon Name="Sophia VirtualCell Workshop">
      <Ribbon.ApplicationMenu>
        <ApplicationMenu>
          <ApplicationMenu.RecentItems>
            <RecentItems CommandName="cmdListRecentItems" />
          </ApplicationMenu.RecentItems>
          <MenuGroup>
            <Button CommandName="cmdButtonRun" />
            <Button CommandName="cmdButtonOpenVirtualCellPackFile" />
          </MenuGroup>
          <MenuGroup>
            <Button CommandName="cmdButtonMenuLicense" />
            <Button CommandName="cmdMenuAbout" />
          </MenuGroup>
          <MenuGroup>
            <Button CommandName="cmdButtonExit" />
          </MenuGroup>
        </ApplicationMenu>
      </Ribbon.ApplicationMenu>
      <Ribbon.QuickAccessToolbar>
        <QuickAccessToolbar>
          <QuickAccessToolbar.ApplicationDefaults>
            <Button CommandName="cmdButtonSaveMenu" ApplicationDefaults.IsChecked="true" />
            <Button CommandName="cmdButtonOpenMenu" ApplicationDefaults.IsChecked="true" />
            <Button CommandName="cmdButtonRun" ApplicationDefaults.IsChecked="true" />
          </QuickAccessToolbar.ApplicationDefaults>
        </QuickAccessToolbar>
      </Ribbon.QuickAccessToolbar>
      <Ribbon.ContextualTabs>
        <TabGroup CommandName="cmdMenuVirtualCellViewer">
          <Tab CommandName="cmdTabVirtualCellBrowser">
            <Group CommandName="cmdGroupVirtualCellBrowser" SizeDefinition="TwoButtons">
              <Button CommandName="cmdButtonCloseVirtualCellPackFile" />
              <Button CommandName="cmdButtonResetNetworkTable" />
            </Group>
            <Group CommandName="cmdGroupDataBrowser" SizeDefinition="ThreeButtons">
              <Button CommandName="cmdButtonViewMassActivityLoads" />
              <Button CommandName="cmdButtonExportMoleculeExpression" />
              <Button CommandName="cmdButtonExportFluxomics" />
            </Group>
            <Group CommandName="cmdGroupBrowserToolkit" SizeDefinition="OneButton">
              <Button CommandName="cmdButtonPhenotypeAnalysis" />
            </Group>
          </Tab>
          <Tab CommandName="cmdTabDataViewer">
            <Group CommandName="cmdGroupDataViewer">
              <Button CommandName="cmdButtonExportPlotMatrix" />
              <CheckBox CommandName="cmdCheckPlotLogScale" />
            </Group>
          </Tab>
        </TabGroup>
        <TabGroup CommandName="cmdMenuWorkbench">
          <Tab CommandName="cmdTabWorkbench">
            <Group CommandName="cmdTabVirtualCellTool">
              <Button CommandName="cmdButtonRun" />
              <Button CommandName="cmdButtonInspectCellModel" />
            </Group>
            <Group CommandName="cmdGroupTools" SizeDefinition="TwoButtons">
              <Button CommandName="cmdButtonEditMutation" />
              <Button CommandName="cmdButtonEditCultureMedium" />
            </Group>
            <Group CommandName="cmdGroupModelling" SizeDefinition="OneButton">
              <Button CommandName="cmdButtonIDAnnotation" />
            </Group>
          </Tab>
        </TabGroup>
        <TabGroup CommandName="cmdGroupModelInspecter">
          <Tab CommandName="cmdTabModelInspecter">
            <Group CommandName="cmdMenuInspector" SizeDefinition="TwoButtons">
              <Button CommandName="cmdButtonPathwayRouter" />
              <Button CommandName="cmdButtonFBATool" />
            </Group>
          </Tab>
        </TabGroup>
      </Ribbon.ContextualTabs>
      <Ribbon.Tabs>
        <Tab CommandName="cmdGroupMainMenu">
          <Group CommandName="cmdTabMainMenu" SizeDefinition="TwoButtons">
            <Button CommandName="cmdButtonOpenMenu" />
            <Button CommandName="cmdButtonSaveMenu" />
          </Group>
          <Group CommandName="cmdGroupOthers" SizeDefinition="OneButton">
            <Button CommandName="cmdMenuAbout" />
          </Group>
        </Tab>
      </Ribbon.Tabs>
    </Ribbon>
    <ContextPopup />
  </Application.Views>
